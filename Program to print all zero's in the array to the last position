class Solution
{
public static void main(String args[]) {
int[] array = {1,2,0,3,4,0,5,0,8};
int x = 0;
for (int i = 0; i < array.length; i++)
 {
if (array[i] != 0) {
array[x] = array[i];
x++;
}
}
while (x < array.length) 
{
array[x] = 0;
x++;
}
for (int i = 0; i <array.length; i++) 
{
System.out.print(array[i]+",");
}
}
}
